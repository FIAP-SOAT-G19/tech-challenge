{
  "name": "tech-challenge",
  "version": "1.0.0",
  "prisma": {
    "seed": "node prisma/seeders/index.js"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed && node -r dotenv/config dist/infra/http/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear --ignore-watch node_modules -r dotenv/config src/infra/http/index.ts dotenv_config_path=.env.dev",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "check": "npx npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage"
  },
