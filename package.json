{
  "name": "tech-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear --ignore-watch node_modules -r dotenv/config src/index.ts dotenv_config_path=.env.dev",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "check": "npx npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test-integration": "npm t -- --config ./jest.integration.config.js",
    "test-integration:watch": "npm run test-integration -- --watch",
    "build-image": "docker build -t tech-challenge:1.0 .",
    "up-logs": "clear && npm run build && docker compose down && docker compose up",
    "up": "clear && npm run build && docker compose down && docker compose up -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FIAP-SOAT-G19/tech-challenge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FIAP-SOAT-G19/tech-challenge/issues"
  },
  "homepage": "https://github.com/FIAP-SOAT-G19/tech-challenge#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@jest-mock/express": "^2.0.1",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/hapi__joi": "^17.1.11",
    "@types/jest": "^29.2.5",
    "@types/joi": "^17.2.3",
    "@types/mockdate": "^3.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.10.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^12.5.0",
    "prisma": "^5.3.1",
    "rimraf": "^4.1.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "mockdate": "^3.0.5",
    "module-alias": "^2.2.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0"
  }
}
